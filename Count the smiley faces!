import java.util.*;

public class SmileFaces {
  
  public static int countSmileys(List<String> arr) {
    String cara = "[:;]([-~]?[)D])"; 

        int contador = 0;

        for (int i = 0; i < arr.size(); i++) { 
            if (arr.get(i).matches(cara)) {
                contador++;
            }
        }

        return contador;
    }
}
