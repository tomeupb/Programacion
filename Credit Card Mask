public class Maskify {
    public static String maskify(String str) {
        
      String vacio = "";
      if(str.length()<=4){
    return str;
}else{
    
    
    for(int i =0 ; i<str.length(); i++){
  if(i< str.length()-4){
    vacio+= "#";
    
    

  }
    }
    
    String visible = str.substring(str.length()-4);
    vacio+=visible;
        return vacio;
    
}
      

    }
}
